{
  "name": "transcribe-stt",
  "version": "1.0.0",
  "description": "Transcribe audio of any length using Google's Speech to Text API",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/index.ts",
    "docs:generate": "api-documenter markdown --input-folder ./docs/input --output-folder ./docs/md",
    "docs:init": "cp ./temp/transcribe-stt.api.json ./docs/input/transcribe-stt.api.json",
    "docs": "npm run build && npm run extract && npm run docs:init && npm run docs:generate && npm run lint:fix",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint": "eslint \"./{src,test}/*.ts\"",
    "extract": "api-extractor run --local --verbose",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "lint": "npm run eslint && npm run prettier",
    "prettier:base": "prettier \"./*({.github,docs,src,test}/**/)*.{json,md}\"",
    "prettier:fix": "npm run prettier:base -- --write",
    "prettier": "npm run prettier:base -- --check",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmmaGoodliffe/transcribe-stt.git"
  },
  "author": "Emma Goodliffe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EmmaGoodliffe/transcribe-stt/issues"
  },
  "homepage": "https://github.com/EmmaGoodliffe/transcribe-stt#readme",
  "keywords": [
    "transcribe",
    "stt",
    "speech-to-text",
    "speech",
    "google",
    "wav"
  ],
  "dependencies": {
    "@google-cloud/speech": "^4.1.1",
    "ora": "^5.0.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.8.25",
    "@microsoft/api-extractor": "^7.9.4",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
