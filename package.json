{
  "name": "speech-to-text",
  "version": "1.0.0",
  "description": "Node app for using Google's Speech to Text API",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node ./src/index.ts",
    "docs:init": "cp ./temp/speech-to-text.api.json ./docs/input/speech-to-text.api.json",
    "docs": "api-documenter markdown --input-folder ./docs/input --output-folder ./docs/md",
    "extract": "api-extractor run --local --diagnostics",
    "lint:fix": "npm run lint -- --fix",
    "lint": "eslint *.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmmaGoodliffe/speech-to-text.git"
  },
  "author": "Emma Goodliffe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EmmaGoodliffe/speech-to-text/issues"
  },
  "homepage": "https://github.com/EmmaGoodliffe/speech-to-text#readme",
  "dependencies": {
    "@google-cloud/speech": "^4.1.1",
    "ora": "^5.0.0"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.8.25",
    "@microsoft/api-extractor": "^7.9.4",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
